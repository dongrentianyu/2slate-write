{
  "description": " A WYSIWYG editor for TiddlyWiki.",
  "license": "MIT",
  "name": "slate-write",
  "version": "0.0.1",
  "url": "https://github.com/tiddly-gittly/slate-write",
  "main": "dist/index.mjs",
  "type": "module",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "npm run build:types && npm run build:js",
    "test": "npm run build:js && NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.json",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "rollup -c",
    "build:demo": "rollup -c rollup.config.demo.js && cp demo/index.html public-dist/index.html",
    "dev:demo": "mkdir -p public-dist/ && cp demo/index.html public-dist/index.html && rollup -c rollup.config.demo.development.js --watch",
    "prepublishOnly": "npm run clean && npm run build",
    "clean": "rimraf dist public-dist",
    "installType": "typesync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/slate-write.git"
  },
  "files": [
    "dist/index.mjs",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "keywords": [
    "generator",
    "template"
  ],
  "author": "lin onetwo",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/slate-write/issues"
  },
  "homepage": "https://github.com/tiddly-gittly/slate-write#readme",
  "dependencies": {
    "slate": "^0.72.8",
    "slate-react": "^0.72.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-modules-commonjs": "^7.16.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.2.5",
    "@types/babel__core": "^7.1.16",
    "@types/babel__preset-env": "^7.9.2",
    "@types/babel-plugin-macros": "^2.8.5",
    "@types/eslint": "7.28.1",
    "@types/eslint-plugin-prettier": "3.1.0",
    "@types/jest": "^27.4.0",
    "@types/lodash": "4.14.175",
    "@types/prettier": "^2.4.1",
    "@types/react": "^17.0.31",
