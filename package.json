    "preval.macro": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-markdown": "^7.1.0",
    "rollup": "^2.27.1",
    "rollup-jest": "^1.1.3",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-serve": "^1.1.0",
    "styled-components": "^5.3.3",
    "typescript": "4.4.4",
    "typesync": "0.8.0",
