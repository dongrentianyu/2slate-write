{
  "description": "A WYSIWYG editor for TiddlyWiki.",
  "tsFiles": [
    "editor/index",
    "event-listeners/modify-navigator"
  ],
  "ignoredExtensionsWhenBuildPlugin": [
    ".ts",
    ".tsx",
    ".snap",
    ".md",
    ".json"
  ],
  "license": "MIT",
  "name": "slate-write",
  "version": "0.0.1",
  "url": "https://github.com/tiddly-gittly/slate-write",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run make && zx scripts/dev/development.mjs",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config=jest.config.js",
    "dev-html": "cross-env CI=true NODE_ENV=production npm run make && npm run run-action && zx ./scripts/build-demo-html.mjs && cd ./dist/output && serve .",
    "make": "npm run clean && npm run build",
    "clean": "rimraf dist",
    "build": "zx scripts/build.mjs && zx scripts/after-build.mjs",
    "run-action": "zx scripts/run-action.mjs",
    "prepublishOnly": "npm run make && tsc --emitDeclarationOnly",
    "build:types": "tsc --emitDeclarationOnly",
    "installType": "typesync"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tiddly-gittly/slate-write.git"
  },
  "files": [
    "dist/index.mjs",
    "dist/**/*.d.ts",
    "README.md"
  ],
  "keywords": [
    "generator",
    "template"
  ],
  "author": "lin onetwo",
  "bugs": {
    "url": "https://github.com/tiddly-gittly/slate-write/issues"
  },
  "homepage": "https://github.com/tiddly-gittly/slate-write#readme",
  "dependencies": {
    "@react-hook/merged-ref": "^1.3.2",
    "@styled-icons/boxicons-regular": "10.47.0",
    "@styled-icons/foundation": "10.46.0",
    "@styled-icons/material": "10.47.0",
    "@tippyjs/react": "4.2.6",
    "@udecode/plate": "16.4.1",
    "downshift": "^6.1.9",
    "html-tags": "3.2.0",
    "lodash": "4.17.21",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "slate": "0.82.0",
    "slate-history": "0.66.0",
    "slate-hyperscript": "0.77.0",
    "slate-react": "0.82.0",
    "tippy.js": "^6.3.7",
    "typescript-styled-is": "^2.1.0",
    "unified": "10.1.2",
